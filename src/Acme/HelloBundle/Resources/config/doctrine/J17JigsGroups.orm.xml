<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Acme\HelloBundle\Entity\J17JigsGroups" table="j17_jigs_groups">
    <unique-constraints>
      <unique-constraint name="gid_5" columns="gid"/>
      <unique-constraint name="id_2" columns="id"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="gid" type="integer" column="gid" nullable="false"/>
    <field name="totalMembers" type="integer" column="total_members" nullable="true"/>
    <field name="totalBank" type="integer" column="total_bank" nullable="true"/>
    <field name="totalXp" type="integer" column="total_xp" nullable="true"/>
    <field name="totalMoney" type="integer" column="total_money" nullable="true"/>
    <field name="coffers" type="integer" column="coffers" nullable="false"/>
  </entity>
</doctrine-mapping>
