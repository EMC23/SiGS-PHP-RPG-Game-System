      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="defence" type="boolean" column="defence" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>

    <field name="idWeapon" type="boolean" column="id_weapon" nullable="false">
      <options>
        <option name="default">2</option>
