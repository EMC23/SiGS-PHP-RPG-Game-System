{% extends 'AppBundle::base.html.twig' %}
{% block body %}
    {{ knp_menu_render('AppBundle:Builder:mainMenu') }}
    {# total items count #}
    <div class="count">

    </div>
    <table id='myTable' width="100%">
        <thead>
        <tr>

            <th> id </th>
            <th> name </th>

            <th> cellwidth</th>
            <th> cellheight</th>
            <th> numberofcells</th>
            <th></th>
        </tr></thead>

        {# table body #}
        {% for article in pagination %}
            <tr {% if loop.index is odd %}{% endif %}>
                <td><a href="/object/{{article.id}}">{{article.id}}</a></td>
                <td><a href="/object/{{article.id}}">{{article.name}}</a></td>
            </tr>
        {% endfor %}








    </table>

    <a href="/monsterTypes/new/">New</a>
    {# display navigation #}
    <div class="navigation">

    </div>
    <script src="/scripts/MonsterTypes.js"></script>
{% endblock %}
